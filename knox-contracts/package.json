{
  "scripts": {
    "prepare": "npx husky install",
    "test": "npm run test:arb",
    "test:eth": "CHAINID=1 npx hardhat test",
    "test:arb": "CHAINID=42161 npx hardhat test",
    "lint-staged": "npx lint-staged",
    "lint:ts": "npx eslint '**/*.ts'",
    "lint:sol": "npx solhint -f table contracts/**/*.sol",
    "lint:ts:fix": "npm run lint:ts --fix",
    "prettier:ts": "npx prettier --write '**/*.ts'",
    "prettier:sol": "npx prettier --write \"contracts/**/*.sol\"",
    "prettier:all": "npm run prettier:ts && npm run prettier:sol",
    "coverage": "rm -rf ./coverage.json ./coverage/ && npx hardhat coverage || exit 0",
    "sizer": "npx hardhat size-contracts",
    "typechain": "npx hardhat clean && npx hardhat typechain"
  },
  "license": "MIT",
  "devDependencies": {
    "@chainlink/contracts": "^0.4.1",
    "@nomiclabs/hardhat-ethers": "^2.0.6",
    "@nomiclabs/hardhat-waffle": "^2.0.2",
    "@premia/utils": "^0.0.4",
    "@solidstate/abdk-math-extensions": "^1.0.0",
    "@solidstate/contracts": "^0.0.35",
    "@solidstate/spec": "^0.0.35",
    "@typechain/ethers-v5": "^9.0.0",
    "@typechain/hardhat": "^4.0.0",
    "@types/chai": "^4.2.18",
    "@types/chai-almost": "^1.0.1",
    "@types/mocha": "^8.2.2",
    "@types/node": "^15.3.0",
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^5.5.0",
    "@uniswap/v2-core": "^1.0.1",
    "@uniswap/v2-periphery": "^1.1.0-beta.0",
    "abdk-libraries-solidity": "^3.0.0",
    "chai": "^4.3.6",
    "chai-almost": "^1.0.1",
    "dotenv": "^10.0.0",
    "eslint": "^8.3.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "ethereum-waffle": "^3.4.0",
    "ethers": "^5.6.0",
    "hardhat": "^2.8.4",
    "hardhat-abi-exporter": "^2.9.0",
    "hardhat-contract-sizer": "^2.5.0",
    "hardhat-dependency-compiler": "^1.1.3",
    "hardhat-docgen": "^1.3.0",
    "hardhat-gas-reporter": "^1.0.8",
    "hardhat-tracer": "^1.0.0-alpha.6",
    "husky": "^8.0.1",
    "lint-staged": "^12.4.1",
    "moment-timezone": "^0.5.34",
    "prettier": "^2.2.1",
    "prettier-plugin-solidity": "1.0.0-beta.10",
    "solhint": "3.3.4",
    "solhint-plugin-prettier": "^0.0.5",
    "solidity-coverage": "^0.7.20",
    "ts-mocha": "^8.0.0",
    "ts-node": "^10.8.1",
    "typechain": "^7.0.0",
    "typescript": "^4.7.3",
    "web3-units": "^1.4.0"
  },
  "lint-staged": {
    "*.ts": "npm run lint:ts",
    "*.sol": "npm run lint:sol"
  }
}
